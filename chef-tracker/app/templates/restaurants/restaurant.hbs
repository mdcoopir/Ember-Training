<div class="w3-container w3-card-4">
  <div class="float-right">Open {{model.yearsOpen}} years </div>
  <h2> {{model.name}}</h2>
  {{#link-to 'restaurants.restaurant.edit' model.id}}<button>Edit</button>{{/link-to}}

  <p>The follwoing chefs serve here:</p>

  <ul>
    {{#each model.chefs as |chef|}}
      <li>
        {{#link-to 'chefs.chef' chef.id}}{{chef.name}}{{/link-to}}
      </li>
    {{/each}}
  </ul>
</div>
